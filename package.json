{
	"name": "component-library",
	"version": "0.2.1",
	"private": true,
	"scripts": {
		"init-githooks": "git config core.hooksPath .githooks",
		"postinstall": "yarn init-githooks || true",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
		"watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
		"start-js": "react-scripts start",
		"start": "npm-run-all -p watch-css start-js",
		"build-js": "react-scripts build",
		"build": "npm-run-all build-css build-js",
		"docs": "yarn build && rm -rf docs && mv build docs",
		"build-commit": "node ./src/build-commit.js",
		"build-package": "[[ -z $(hash webpack) ]] && yarn build-css && webpack --mode='development' --progress --config ./src/webpack.package.config.js",
		"version-bump": "node ./src/version-bump.js",
		"package": "yarn version-bump && yarn build-package && yarn build-commit"
	},
	"devDependencies": {
		"axios": "^0.18.0",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-loader": "^7.1.5",
		"babel-plugin-react-css-modules": "^3.4.2",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"babel-preset-env": "^1.7.0",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-2": "^6.24.1",
		"classnames": "^2.2.6",
		"colors": "^1.3.2",
		"css-loader": "^1.0.0",
		"custom-react-scripts": "^0.2.2",
		"emoji-regex": "^7.0.0",
		"enzyme": "^3.3.0",
		"enzyme-adapter-react-16": "^1.1.1",
		"eslint": "^4.1.1",
		"eslint-config-react-app": "^2.1.0",
		"eslint-plugin-flowtype": "^2.50.0",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-jsx-a11y": "^5.1.1",
		"eslint-plugin-react": "^7.11.1",
		"flow-bin": "^0.75.0",
		"html-webpack-plugin": "^3.2.0",
		"http-status-codes": "^1.3.0",
		"i18next": "^11.5.0",
		"i18next-browser-languagedetector": "^2.2.3",
		"js-cookie": "^2.2.0",
		"js-md5": "^0.7.3",
		"lorem-ipsum": "^1.0.5",
		"ls": "^0.2.1",
		"material-design-icons": "^3.0.1",
		"mock-local-storage": "^1.0.5",
		"node-sass-chokidar": "^1.3.0",
		"npm-run-all": "^4.1.3",
		"query-string": "5",
		"react": "^16.4.1",
		"react-dom": "^16.4.1",
		"react-i18next": "^7.10.1",
		"react-redux": "^5.0.7",
		"react-resize-detector": "^3.0.1",
		"react-router-dom": "^4.3.1",
		"react-select": "^2.0.0-beta.7",
		"react-spinkit": "^3.0.0",
		"react-svg-loader": "^2.1.0",
		"react-syntax-highlighter": "^8.0.1",
		"react-test-renderer": "^16.4.2",
		"redux": "^4.0.0",
		"redux-devtools-extension": "^2.13.5",
		"redux-form": "^7.4.2",
		"redux-immutable-state-invariant": "^2.1.0",
		"redux-saga": "^0.16.0",
		"storage.io": "^1.2.0",
		"style-loader": "^0.22.1",
		"voca": "^1.4.0",
		"webpack": "^4.17.0",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.5"
	},
	"description": "A `create-react-app` based frontend starter project, using `custom-react-scripts`.",
	"main": "./src/index.package.generated.js",
	"license": "MIT",
	"homepage": "https://shuttl-tech.github.io/component-library/"
}
