{
  "name": "component-library",
  "version": "1.0.0",
  "description": "Shuttl Frontend Component Library",
  "author": "jayantbh",
  "license": "MIT",
  "repository": "https://github.com/Shuttl-Tech/component-library",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --write \"{src,example,.storybook}/**/*.{ts,tsx,js,scss}\"",
    "check-format": "prettier --check \"{src,example,.storybook}/**/*.{ts,tsx,js,scss}\""
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "classnames": "^2.2.6"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-info": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "@storybook/theming": "^5.2.1",
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/storybook__react": "^4.0.2",
    "autoprefixer": "^9.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.2.0",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react": "^16.4.1",
    "react-docgen-typescript-loader": "^3.2.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^3.1.2",
    "react-scripts-ts": "^2.16.0",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-includepaths": "^0.2.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-typescript2": "^0.17.0",
    "rollup-plugin-url": "^1.4.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3"
  },
  "files": [
    "dist"
  ]
}
